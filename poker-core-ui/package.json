    "@vue/tsconfig": "^0.1.3",
    "eslint": "^8.34.0",
    "eslint-plugin-vue": "^9.9.0",
    "jsdom": "^21.1.0",
    "msw": "^2.1.7",
    "npm-run-all": "^4.1.5",
    "prettier": "^2.8.4",
    "resize-observer-polyfill": "^1.5.1",
    "typescript": "~4.8.4",
    "vite": "^4.1.4",
    "vite-plugin-vuetify": "^1.0.2",
    "vite-tsconfig-paths": "^4.2.1",
    "vitest": "^0.34.6",
    "vue-tsc": "^1.2.0"
  }
